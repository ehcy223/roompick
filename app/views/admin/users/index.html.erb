<h1 class="h5 mb-3">会員一覧</h1>
<table class="table table-sm">
  <thead><tr><th>ID</th><th>名前/メール</th><th>登録日</th></tr></thead>
  <tbody>
    <% @users.each do |u| %>
      <tr>
        <td><%= u.id %></td>
        <td><%= link_to (u.name.presence || u.email), admin_user_path(u) %></td>
        <td><%= l(u.created_at.to_date) rescue u.created_at.to_date %></td>
      </tr>
    <% end %>
  </tbody>
</table>